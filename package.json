{
  "name": "vue-h5-template",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite  --force",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "deps": "yarn upgrade-interactive --latest",
    "commit": "git add . && git-cz"
  },
  "dependencies": {
    "@rollup/plugin-inject": "^5.0.3",
    "@splinetool/runtime": "^0.9.437",
    "@types/jquery": "^3.5.16",
    "@varlet/ui": "^2.11.4",
    "@vueuse/core": "10.1.2",
    "@vueuse/integrations": "10.1.2",
    "axios": "1.4.0",
    "better-scroll": "^2.5.1",
    "chart.js": "^2.7.3",
    "dayjs": "^1.11.8",
    "gsap": "3.8.0",
    "jquery": "^3.7.0",
    "jquery-contextmenu": "^2.9.2",
    "mitt": "^3.0.0",
    "pinia": "^2.1.0",
    "pinia-plugin-persistedstate": "^3.1.0",
    "react-dom": "^17.0.2",
    "sass": "^1.63.5",
    "solid-js": "^1.7.12",
    "three": "0.152",
    "typed.js": "^2.0.12",
    "universal-cookie": "^4.0.4",
    "vanilla-tilt": "^1.7.0",
    "veaury": "^2.3.8",
    "vite-plugin-mkcert": "^1.16.0",
    "vue": "^3.3.1",
    "vue-hooks-api": "^0.0.0-beta.5",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^3.6.1",
    "@types/node": "^17.0.21",
    "@types/three": "^0.152.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@unocss/eslint-config": "^0.56.4",
    "@unocss/extractor-pug": "^0.56.4",
    "@unocss/preset-uno": "^0.56.4",
    "@vitejs/plugin-legacy": "^4.0.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "^10.4.14",
    "bin-wrapper": "npm:bin-wrapper-china",
    "cnjm-postcss-px-to-viewport": "^1.0.0",
    "consola": "^3.1.0",
    "cross-env": "^7.0.3",
    "eruda": "^3.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "gifsicle": "5.2.0",
    "git-cz": "^4.9.0",
    "husky": "8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "13.2.2",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-html": "1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^15.7.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "terser": "^5.17.7",
    "typescript": "^5.0.0",
    "unocss": "^0.56.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eruda": "^1.0.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-restart": "^0.3.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend-plus": "^0.1.0",
    "vitest": "^0.32.0",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.6.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
